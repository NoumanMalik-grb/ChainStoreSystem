@model IEnumerable<ChainStoreSystem.Models.Order>
@{
    ViewData["Title"] = "StockReport";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h1 class="text-center" style="font-family:'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif">StockReport</h1>
<hr />
@using (Html.BeginForm("StockReport", "StoreReport", FormMethod.Post))
{<table class="table table-responsive">
        <tr>
            <td>Date From:</td>
            <td><input name="DateFrom" type="datetime-local" value=@ViewBag.DateFrom class="form-control" /></td>
            <td>Date To:</td>
            <td><input name="DateTo" type="datetime-local" value=@ViewBag.DateTo class="form-control" /></td>

        </tr>
        <tr>
            <td>category:</td>
            <td>
                @Html.DropDownList("SubCategory", null, "Salect category", htmlAttributes: new { @class = "form-control" })
            </td>
            <td>Product:</td>
            <td>
                @Html.DropDownList("Product", null, "Salect Poduct", htmlAttributes: new { @class = "form-control" })
            </td>
            <td>
                <button class="btn  btn-block" style="background-color:#ff6666; color:white">Generate Report</button>
            </td>
        </tr>
    </table>
}


